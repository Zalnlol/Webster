@model IEnumerable<WebsterWebApp.Models.Exam>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<style>
.A_1 {
    block-size: 20px;
    border-block-end-color: rgb(255, 255, 255);
    border-block-start-color: rgb(255, 255, 255);
    border-collapse: collapse;
    border-end-end-radius: 5px;
    border-end-start-radius: 5px;
    border-inline-end-color: rgb(255, 255, 255);
    border-inline-start-color: rgb(255, 255, 255);
    border-start-end-radius: 5px;
    border-start-start-radius: 5px;
    box-sizing: border-box;
    caret-color: rgb(255, 255, 255);
    color: rgb(255, 255, 255);
    column-rule-color: rgb(255, 255, 255);
    display: block;
    float: right;
    height: 27px;
    inline-size: 55.3594px;
    padding-block-end: 2px;
    padding-block-start: 1px;
    padding-inline-end: 10px;
    padding-inline-start: 10px;
    perspective-origin: 27.6719px 13.5px;
    text-align: center;
    text-decoration: none solid rgb(255, 255, 255);
    text-size-adjust: 100%;
    transform-origin: 27.6797px 13.5px;
    user-select: none;
    vertical-align: middle;
    white-space: nowrap;    
    width: 75.3594px;
    background: rgb(4, 170, 109) none repeat scroll 0% 0% / auto padding-box border-box;
    border: 0px none rgb(255, 255, 255);
    border-radius: 5px;
    font: 16px / 24px "Source Sans Pro", sans-serif;
    outline: rgb(255, 255, 255) none 0px;
    overflow: hidden;
    padding: 1px 10px 2px;
}/*#A_1*/

.A_1:after {
    border-block-end-color: rgb(255, 255, 255);
    border-block-start-color: rgb(255, 255, 255);
    border-collapse: collapse;
    border-inline-end-color: rgb(255, 255, 255);
    border-inline-start-color: rgb(255, 255, 255);
    box-sizing: border-box;
    caret-color: rgb(255, 255, 255);
    color: rgb(255, 255, 255);
    column-rule-color: rgb(255, 255, 255);
    text-align: center;
    text-decoration: none solid rgb(255, 255, 255);
    text-size-adjust: 100%;
    user-select: none;
    white-space: nowrap;
    border: 0px none rgb(255, 255, 255);
    font: 16px / 24px "Source Sans Pro", sans-serif;
    outline: rgb(255, 255, 255) none 0px;
}/*#A_1:after*/

.A_1:before {
    border-block-end-color: rgb(255, 255, 255);
    border-block-start-color: rgb(255, 255, 255);
    border-collapse: collapse;
    border-inline-end-color: rgb(255, 255, 255);
    border-inline-start-color: rgb(255, 255, 255);
    box-sizing: border-box;
    caret-color: rgb(255, 255, 255);
    color: rgb(255, 255, 255);
    column-rule-color: rgb(255, 255, 255);
    text-align: center;
    text-decoration: none solid rgb(255, 255, 255);
    text-size-adjust: 100%;
    user-select: none;
    white-space: nowrap;
    border: 0px none rgb(255, 255, 255);
    font: 16px / 24px "Source Sans Pro", sans-serif;
    outline: rgb(255, 255, 255) none 0px;
}/*#A_1:before*/

</style>
<p>
    <a asp-action="Create">Create New</a>
</p>
<p class="text-danger">@ViewBag.msg</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExamName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PassPercent)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FinishTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PassPercent)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FinishTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ExamId" class="A_1">Edit</a>

            </td>
            <td>

                <a asp-action="UserList" asp-route-id="@item.ExamId" class="A_1">UserList</a>

            </td>
            <td>

                <a asp-action="Details" asp-route-id="@item.ExamId" class="A_1">Details</a>
            </td>
            <td>
                <a asp-action="Delete" asp-route-id="@item.ExamId" class="A_1">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
